#include<iostream>
using namespace std;

int main(){
int t;
cin>>t;
while(t--){
    int a,b,c,d;
    cin>>a>>b>>c>>d;
    int sum=a+b+c;
    if(sum<=d){
        cout<<"1"<<endl;
    }
    else {
      if(a+b>d&&b+c>d&&c+a>d){
          cout<<"3"<<endl;
      }
      else{
          cout<<"2"<<endl;
      }

    }
    
}
return 0;
}
